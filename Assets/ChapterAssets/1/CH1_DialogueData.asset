      animation: 0
      shapekey: 0
    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 2
    text: No one would talk to this ugliest of fish,
    audioClip: {fileID: 8300000, guid: 17bafd5ae6d144194a07071c96497b64, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 
    nonSpeakingCharacterActions:
