    audioClip: {fileID: 8300000, guid: f65477689fde44e36bb90d756a5c2217, type: 3}
    animationName: 0
    shapekeyName: 3
    prefabIndices: 
    nonSpeakingCharacterActions:
    - character: 2
      animation: 0
      shapekey: 0
    - character: 1
      animation: 0
      shapekey: 0
  - speaker: 0
