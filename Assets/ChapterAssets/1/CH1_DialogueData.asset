      shapekey: 15
    - character: 1
      animation: 0
      shapekey: 0
    - character: 3
      animation: 0
      shapekey: 0
    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 2
    text: Alone he swam in the river all day,
    audioClip: {fileID: 8300000, guid: f73d3f837c14a46abab2551d3a32da75, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 
