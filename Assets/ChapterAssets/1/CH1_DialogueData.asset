    - character: 2
      animation: 0
      shapekey: 0
    - character: 3
      animation: 0
      shapekey: 0
    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 0
    text: "With jaws that are as powerful as a boxer\u2019s fist."
    audioClip: {fileID: 8300000, guid: faf620104ac634b2f99f4fe564e7a8de, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 00000000
