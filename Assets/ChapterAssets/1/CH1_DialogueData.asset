      animation: 0
      shapekey: 0
    - character: 2
      animation: 0
      shapekey: 0
    - character: 3
      animation: 0
      shapekey: 0
    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 0
    text: I wish this misery would come to an end.
    audioClip: {fileID: 8300000, guid: 3bb2a4ad9fc9e492ba2c6275cae352f4, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 
    nonSpeakingCharacterActions:
    - character: 1
