    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 2
    text: When he tried to make friends, all the animals would flee.
    audioClip: {fileID: 8300000, guid: 7b9504a7f4d8f4eb6bfa74cc1dc51a3b, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 
