    - character: 1
      animation: 0
      shapekey: 0
    - character: 3
      animation: 0
      shapekey: 0
    - character: 4
      animation: 0
      shapekey: 0
    - character: 5
      animation: 0
      shapekey: 0
  - speaker: 2
    text: Without a single friend to come and play.
    audioClip: {fileID: 8300000, guid: b2899f738d37a4d9e87c07ad9e3d446d, type: 3}
    animationName: 0
    shapekeyName: 0
    prefabIndices: 
